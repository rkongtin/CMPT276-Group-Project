<div style="background-color:white">
    <h1>Settings Page</h1>
    <div class="profile-pic" style="background-image:url(<%= @user.pictures.url(:small) %>)"></div>
    <div class="profile">
        <p class="profile"><%= link_to "Change Email", changeEmail_user_path, class: "profile" %></p>
        <p class="profile"><%= link_to "Change Password", changePassword_user_path, class: "profile" %></p>
        <p class="profile"><%= link_to "Change All Information", edit_user_path(current_user), class: "profile" %></p>
    </div>
    <div style="clear:both"></div>
    <br /><br /><br />
</div>
